{
    "packageRules": [
      {
        "matchPackagePatterns": ["eslint"],
        "labels": ["linting"]
      },
      {
        "matchDepTypes": ["optionalDependencies"],
        "addLabels": ["optional"]
      },
      {
        "matchUpdateTypes": ["minor", "patch", "pin", "digest"],
        "automerge": true
      }
    ],
    "extends": [
        "config:base"
    ]
}
